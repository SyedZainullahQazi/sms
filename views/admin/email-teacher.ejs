  <body>
    <%-include("../partials/admin/header")-%>
    <div>&nbsp;</div>

    <div>&nbsp;</div>

    <div>&nbsp;</div>

    <div class="container" id="listing">
      <h3>EMAIL TEACHER</h3>
      <table class="table table-striped">
        <thead>
          <tr>
            <th>Teacher Name</th>
            <th>Email</th>
            <th>Designation</th>
          </tr>
        </thead>
        <tbody>
          <%teacher.forEach(function(element){%>
          <tr>
            <td><%=element.name%></td>
            <td><%=element.email%></td>
            <td><%=element.designation%></td>
            <td>
              <!--Roll no-->
              <form method="post" action="/mailinfo" style="display:inline">
              <button value=<%=element.email%> name="email" type="submit" class="btn btn-success" >Email</button>
              </form>
            </td>
          </tr>
          <%})%>
          <tr>
            <td>Syed Zainullah Qazi</td>
            <td>f200328@cfd.nu.edu.pk</td>
            <td>VICE PRESIDENT FCAP</td>
            <td>
              <!--Roll no-->
              <form method="post" action="/mailinfo" style="display:inline">
              <button value=f200328@cfd.nu.edu.pk name="email" type="submit" class="btn btn-success" >Email</button>
              </form>
            </td>
          </tr>
        </tbody>
      </table>
    </div>

    <%-include("../partials/admin/footer")-%>
